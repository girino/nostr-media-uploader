[telegram]
# Bot token from @BotFather
bot_token = YOUR_BOT_TOKEN_HERE

# Your Telegram user ID (get from @userinfobot)
owner_id = YOUR_TELEGRAM_USER_ID

# Chat ID (channel/group ID or @channelname)
# Get numeric ID from @RawDataBot or use @channelname format
chat_id = YOUR_CHAT_ID

# Profile name to use with nostr_media_uploader.sh
profile_name = tarado

# Path to nostr_media_uploader.sh script
script_path = ./nostr_media_uploader.sh

